{
  "name": "streamoid-catalog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "cleanup": "node scripts/cleanup.js",
    "cleanup:all": "node scripts/cleanup.js --all"
  },
  "dependencies": {
    "csv-parse": "^5.3.2",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "pg-format": "^1.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/multer": "^1.4.7",
    "@types/pg": "^8.15.5",
    "@types/pg-format": "^1.0.3",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  }
}
